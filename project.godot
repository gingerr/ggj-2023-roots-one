config_version=5

[application]

config/name="shRooter"
run/main_scene="res://MainMenu.tscn"
config/features=PackedStringArray("4.0")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://resources/splash.png"
boot_splash/fullsize=false
config/icon="res://player/spaceship1.png"
config/windows_native_icon="res://resources/shRooter.ico"

[autoload]

HUD="*res://HUD.tscn"

[debug]

settings/stdout/print_fps=true

[display]

window/size/viewport_width=1400
window/size/viewport_height=800
window/size/mode=4
window/size/resizable=false
window/size/borderless=true
window/subwindows/embed_subwindows=false
window/size/fullscreen=true
window/vsync/use_vsync=true

[global]

vsync=true

[gui]

theme/custom="res://new_theme.tres"
theme/custom_font="res://resources/retro-gaming.ttf"
common/drop_mouse_on_gui_input_disabled=true

